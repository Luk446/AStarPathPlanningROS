<?xml version="1.0"?>
<launch>

  <!-- Path planner node -->
  <node name="astar_planner" pkg="rosbot_navigation" type="bfs_planner_grid.py" output="screen"/>

  <!-- Path follower node -->
<node name="path_follower" pkg="rosbot_navigation" type="path_follower.py" output="screen">
  <param name="start_delay" value="2.0"/>
</node>



</launch>

